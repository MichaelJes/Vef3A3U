]}';
b)
$json_array = json_decode($jsonData, true);
array_push(json_array,array(       //add the employee
    title:"Mountain",
    url:"http://kingofwallpapers.com/1920x1080/1920x1080-008.jpg",)
);
$jsonData = json_encode($json_array);
//file_put_contents($jsonDatan, $jsonData);
c)

$json_array = json_decode($jsonData, true);
foreach ($json_array as $value) {
	echo $value['title'];
	echo '<img src=",$value['url']," alt=",$value['title']," height="100" width="100">'
}

